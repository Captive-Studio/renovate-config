{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Automerge des updates de digest Dockerfiles",
      "matchManagers": ["dockerfile", "circleci", "docker-compose"],
      "matchUpdateTypes": ["digest", "patch"],
      "automerge": true
    }
  ]
}
