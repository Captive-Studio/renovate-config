{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Automerge Test packages",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true,
      "matchPackageNames": ["rspec"],
      "matchPackagePatterns": ["^rspec-"]
    }
  ]
}
